{
    "routes": [
            { "src": "/api/(.*)", "dest": "/main.py" },
            { "src": "/(.*)", "dest": "/index.html" }  
    ],
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/(.*)", "has": [ { "type": "route", "value": "/api/(.*)" } ] },
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }
  