{
    "routes": [
            { "src": "/(.*)", "dest": "/main.py" },
            { "src": "/(.*)", "dest": "/index.html" }  
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/(.*)", "has": [ { "type": "route", "value": "/(.*)" } ] },
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }
  